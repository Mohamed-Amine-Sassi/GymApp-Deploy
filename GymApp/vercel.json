{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist" } 
    }
  ],
  "routes": [
    {
      
      "dest": "https://gym-app-backend-jet.vercel.app",
      "methods": ["GET", "POST", "PUT", "DELETE","PATCH"]
    },
    {
      "src": "/(.*)", 
      "dest": "/index.html", 
      "status": 200
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { 
          "key": "Access-Control-Allow-Origin", 
          "value": "https://gym-app-backend-jet.vercel.app" 
        },
        { 
          "key": "Access-Control-Allow-Credentials", 
          "value": "true" 
        }
      ]
    }
  ]
}